<!-- Hovercardに表示するDB選択 -->
<div style="display: none;">
    <form>
        <input type="checkbox" class="selected_db" name="targetdb" value="dbpedia" checked>DBpedia
        <input type="checkbox" class="selected_db" name="fa" value="fa" checked>FirstAuthor検索
