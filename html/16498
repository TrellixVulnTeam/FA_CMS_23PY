        $('#search_val').keypress(function (e) {
            if (e.keyCode == 13) {
                e.preventDefault()
                var url = "./search.htm?val=" + $("#search_val").val();
                location.href = url
            }
        });
        $("#searchsubmit").click(function () {
            var url = "./search.htm?val=" + $("#search_val").val();
